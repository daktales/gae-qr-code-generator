         xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
         http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5">
  <servlet>
    <servlet-name>QRCodeGeneratorAPI</servlet-name>
    <servlet-class>it.daktales.gae.apps.gaeqrcodegeneratorapi.QRCodeGeneratorAPI</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>QRCodeGeneratorAPI</servlet-name>
    <url-pattern>/qrcode/*</url-pattern>
  </servlet-mapping>
</web-app>
